<UserControl x:Class="ChartCPNTs.Views.Mesh"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChartCPNTs.Views"
             xmlns:vm="clr-namespace:ChartCPNTs.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             SizeChanged="sizeChangedEvent">
    <UserControl.DataContext>
        <vm:MeshViewModel x:Name="ViewModel"/>
    </UserControl.DataContext>
    <Grid Name="grid" Margin="5"/>
</UserControl>
